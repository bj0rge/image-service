{
  "name": "image-cache",
  "version": "1.0.0",
  "description": "Service handling images cache",
  "main": "./dist/index.js",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/multipart": "^9.0.1",
    "config": "^3.3.12",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "fastify-tsconfig": "2.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "glob": "^11.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@faker-js/faker": "^9.2.0",
    "@types/config": "^3.3.5",
    "@types/node": "22.7.4",
    "pino-pretty": "^11.3.0",
    "ts-node": "^10.9.2",
    "tsx": "4.7.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build:ts": "tsc",
    "build:babel": "babel ./dist --out-dir ./dist --extensions \".js\"",
